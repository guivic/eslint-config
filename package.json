{
  "name": "@guivic/eslint-config-guivic",
  "version": "0.2.0",
  "description": "Offical Guivic javascript norm that extends the Airbnb style.",
  "main": "main.js",
  "files": [
    "main.js",
    ".eslintrc.json"
  ],
  "scripts": {
    "prepublish": "safe-publish-latest || (npm test && eslint-find-rules --unused)",
    "lint": "eslint . --env node",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guivic/eslint-config.git"
  },
  "keywords": [
    "guivic",
    "estlint-config",
    "estlint-config-guivic",
    "guivic-eslint-config"
  ],
  "author": "lludol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guivic/eslint-config/issues"
  },
  "homepage": "https://github.com/guivic/eslint-config#readme",
  "peerDependencies": {
    "eslint": "8.x"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-find-rules": "^4.1.0",
    "safe-publish-latest": "^2.0.0"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0"
  }
}
